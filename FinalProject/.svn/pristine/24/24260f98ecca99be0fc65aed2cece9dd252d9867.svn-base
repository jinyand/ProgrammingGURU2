package com.example.user.finalproject.bean;

import java.util.ArrayList;
import java.util.List;

/**
 * Created by user on 2018-07-27.
 */

public class SaveBean extends com.example.user.finalproject.bean.CommonBean {


    private LostBean lostBean;
    private FoundBean foundBean;


    // FoundBean의 정보를 List로
    private List<com.example.user.finalproject.bean.FoundBean> foundBeanList = new ArrayList<com.example.user.finalproject.bean.FoundBean>();

    // LostBean의 정보를 List로
    private List<com.example.user.finalproject.bean.LostBean> lostBeanList = new ArrayList<com.example.user.finalproject.bean.LostBean>();


    public FoundBean getFoundBean() {
        return foundBean;
    }

    public void setFoundBean(FoundBean foundBean) {
        this.foundBean = foundBean;
    }

    public LostBean getLostBean() {
        return lostBean;
    }

    public void setLostBean(LostBean lostBean) {
        this.lostBean = lostBean;
    }

    public List<com.example.user.finalproject.bean.FoundBean> getFoundBeanList() {
        return foundBeanList;
    }

    public void setFoundBeanList(List<FoundBean> foundBeanList) {
        this.foundBeanList = foundBeanList;
    }

    public List<com.example.user.finalproject.bean.LostBean> getLostBeanList() {
        return lostBeanList;
    }

    public void setLostBeanList(List<LostBean> lostBeanList) {
        this.lostBeanList = lostBeanList;
    }
}
