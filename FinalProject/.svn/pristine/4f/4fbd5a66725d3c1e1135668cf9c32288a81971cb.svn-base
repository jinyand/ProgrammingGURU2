package com.example.user.finalproject.bean;

import com.google.android.gms.maps.model.LatLng;

import java.util.ArrayList;
import java.util.List;

/**
 * Created by user on 2018-07-27.
 * 습득한 분실물 정보 Bean
 */

public class FoundBean extends CommonBean {

    // list에서 현재 내 Row의 Index를 저장
    private int selIdx;

    private String imgPath;
    // 글 제목
    private String title;
    // 글 내용
    private String content;
    private String edtPwd;
    private double latitude;
    private double longitude;

    private List<FCBean> fcBeanList = new ArrayList<FCBean>();



    public List<FCBean> getFcBeanList() {
        return fcBeanList;
    }

    public void setFcBeanList(List<FCBean> fcBeanList) {
        this.fcBeanList = fcBeanList;
    }



    public double getLatitude() {
        return latitude;
    }
    public void setLatitude(double latitude) {
        this.latitude = latitude;
    }
    public double getLongitude() {
        return longitude;
    }
    public void setLongitude(double longitude) {
        this.longitude = longitude;
    }
    public String getEdtPwd() {
        return edtPwd;
    }

    public void setEdtPwd(String edtPwd) {
        this.edtPwd = edtPwd;
    }

    public String getImgPath() {
        return imgPath;
    }

    public void setImgPath(String imgPath) {
        this.imgPath = imgPath;
    }

    public int getSelIdx() {
        return selIdx;
    }

    public void setSelIdx(int selIdx) {
        this.selIdx = selIdx;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

}
